{
	"name" : "bitworkz/adyen",
	"description" : "Simplified package to work with Adyen",
	"type" : "package",
	"require" : {
		"php" : ">=7.1.0",
		"adyen/php-api-library" : "~1.4"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~7.1"
	},
	"license" : "mit",
	"authors" : [{
			"name" : "Erik Willems",
			"email" : "contact@bitworkz.com",
			"homepage" : "https://www.bitworkz.com"
		}
	],
	"minimum-stability" : "dev",
	"autoload" : {
		"psr-4" : {
			"bitworkz\\Adyen\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"bitworkz\\Adyen\\Test\\" : "tests/"
		}
	},
	"keywords" : [
		"adyen",
		"bitworkz"
	]
}